{
    "containerDefinitions": [
        {
            "name": "chatbot",
            "image": "${IMAGE_NAME}:${TAG}",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "chatbot-8501-tcp",
                    "containerPort": 8501,
                    "hostPort": 8501,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "secrets": [
                {
                  "name": "OPENAI_API_KEY",
                  "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:618044871166:secret:OPEN_AI_API-scjYEb"
                }
              ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/Chatbot_ARM",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "Chatbot_ARM",
    "executionRoleArn": "arn:aws:iam::618044871166:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "ARM64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
    
}